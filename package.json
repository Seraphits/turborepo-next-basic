{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test:e2e": "pnpm turbo run dev",
    "dev:clean": "kill-port 3002 3001 || true && turbo run dev",
    "prepare": "husky",
    "test": "turbo run test"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "stylelint": "^16.26.1",
    "turbo": "^2.6.1",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=22.12.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "husky",
      "@next/swc-linux-x64-gnu",
      "@next/swc-linux-x64-musl"
    ]
  }
}
